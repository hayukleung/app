<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

  <com.hayukleung.app.view.Header
      android:id="@+id/header"
      android:layout_width="match_parent"
      android:layout_height="@dimen/header_height"
      />

  <com.hayukleung.collapsibleview.CollapsibleView
      android:id="@+id/ActivityMain$collapsible_view"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:layout_below="@+id/header"
      app:drawableClosed="@null"
      app:drawableOpened="@null"
      app:drawableOrg="@null"
      app:drawableUsr="@null"
      />
</RelativeLayout>
